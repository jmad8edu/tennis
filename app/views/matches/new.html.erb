<h1>New Match</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@match) do |f| %>
      <%= render 'shared/error_messages', object: @match %>
      <%= render 'fields', f: f %>
      <div class="row">
      	<%= f.submit "Send challenge", class: "btn btn-large btn-primary" %>
      </div>
    <% end %>
  </div>
</div>

