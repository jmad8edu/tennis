<h1>New Match</h1>

<div class="row">
  <div class="col-md-8 col-md-offset-2 col-xs-12">
    <%= form_for(@match) do |f| %>
      <%= render 'shared/error_messages', object: @match %>
      <%= render 'fields', f: f %>
      <div class="row">
      	<%= f.submit "Send challenge", class: "btn btn-large btn-primary" %>
      </div>
    <% end %>
  </div>
</div>

